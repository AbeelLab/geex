<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.Group?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<fx:root id="newickSelectContainer" type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">

    <ScrollPane styleClass="transparent" fx:id="newickScroller" AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0"
                AnchorPane.bottomAnchor="0"
                AnchorPane.leftAnchor="0" fitToHeight="true" fitToWidth="true">
        <StackPane>
            <StackPane styleClass="margin">
                <Group fx:id="newickContainer"/>
            </StackPane>
            <ProgressIndicator fx:id="progressIndicator"/>
        </StackPane>
    </ScrollPane>

    <Label styleClass="header-text" AnchorPane.topAnchor="0" AnchorPane.rightAnchor="0"
           AnchorPane.leftAnchor="0">
        Select strains from the Phylogenetic tree
    </Label>

    <HBox fx:id="search" AnchorPane.topAnchor="30" AnchorPane.rightAnchor="40"/>

</fx:root>
