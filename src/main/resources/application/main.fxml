<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<fx:root type="javafx.scene.layout.BorderPane" xmlns:fx="http://javafx.com/fxml">

    <center>
        <ScrollPane>
            <StackPane>
                <GridPane>
                    <HBox fx:id="ruler" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                    <GridPane fx:id="sequences" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
                </GridPane>
                <ProgressIndicator fx:id="progressIndicator"/>
            </StackPane>
        </ScrollPane>
    </center>

    <stylesheets>
        <URL value="@css/style.css"/>
    </stylesheets>

</fx:root>
